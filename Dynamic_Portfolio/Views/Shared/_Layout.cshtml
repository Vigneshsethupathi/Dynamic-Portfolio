<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - porfolio</title>
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.7.0/css/font-awesome.min.css">
    <link rel="stylesheet" href="~/css/site.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/Dynamic_Portfolio.styles.css" asp-append-version="true" />
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap-icons@1.11.3/font/bootstrap-icons.min.css">
    <link rel="stylesheet" href="https://cdn.datatables.net/2.0.8/css/dataTables.dataTables.css" />
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.0.0-beta3/css/all.min.css">
    <script src="https://kit.fontawesome.com/43d7636c49.js" crossorigin="anonymous"></script>
</head>
<body>
    <partial name="_notification" />
    <div class="portfolio_main_Div" >
    <header>
        
            <nav id=navtag>
                <a class="Portfolio_Logo" asp-area="" asp-controller="Home" asp-action="Index"><i class="bi bi-house-door" style=" text-shadow: 0px 0px 5px yellowgreen; color:black" ></i> Portfolio</a>
               
                <div class="content_menus">
                    <ul>
                        <li>
                        <a asp-area="" asp-controller="Home" asp-action="Project"><i class="bi bi-journal-richtext"></i> Projects</a>
                        </li>
                        <li>
                        <a asp-area="" asp-controller="Home" asp-action="Skills"><i class="bi bi-diagram-3"></i> Skill's</a>
                        </li>
                        <li>
                        <a asp-area="" asp-controller="Home" asp-action="Contact"><i class="bi bi-telephone"></i> Contact</a>
                        </li>
                        <li>
                        <a asp-area="" asp-controller="Home" asp-action="About"><i class="bi bi-file-person"></i> about</a>
                        </li>
                    </ul>
                </div>
        </nav>
    </header>
    
       @RenderBody()
   
    <footer class="project_footer">
            <div>
               <b>P</b>owerd</b> by <u>Vigneshsethupathi2001</u>
                <div class="footer_icons">
                    <a href="" title="FaceBook"><i class="bi bi-facebook"></i></a>
                    <a href="" title="Linkedin"><i class="bi bi-linkedin" style=""></i></a>
                    <a href="" title="Whatsapp"><i class="bi bi-whatsapp"></i></a>
                    <a href="" title="Instagram"><i class="bi bi-instagram"></i></a>
                </div>
            </div>
              
    </footer>
    
    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>
        <script src="//cdnjs.cloudflare.com/ajax/libs/toastr.js/latest/toastr.min.js"></script>
        <link rel="stylesheet" href="//cdnjs.cloudflare.com/ajax/libs/toastr.js/latest/toastr.min.css" />
        <script src="https://cdn.datatables.net/2.0.8/js/dataTables.js"></script>
    @await RenderSectionAsync("Scripts", required: false)
    </div>

   
</body>
</html>
